{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "",
            "arch": "",
            "cache_duration": "none",
            "centos": "",
            "debian": "",
            "elementary": "",
            "fedora": "",
            "gentoo": "",
            "linux": "",
            "macos": "",
            "manjaro": "",
            "mint": "",
            "opensuse": "",
            "raspbian": "",
            "ubuntu": "",
            "windows": ""
          },
          "leading_diamond": "╭─",
          "template": " {{ .Icon }} ",
          "foreground": "#f2f3f8",
          "background": "#d75f00",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }} ",
          "foreground": "#4e4e4e",
          "powerline_symbol": "",
          "background": "#e4e4e4",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "…",
            "folder_separator_icon": " <transparent></> ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#f2f3f8",
          "powerline_symbol": "",
          "background": "#0087af",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_gone_icon": "",
            "branch_icon": " ",
            "branch_identical_icon": "",
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "leading_diamond": "<transparent,background></>",
          "trailing_diamond": "",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent></> <#121318> {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent></> <#121318> {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent></> <#121318> {{ .StashCount }}</>{{ end }} ",
          "foreground": "#f2f3f8",
          "background": "#378504",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#a97400{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#54433a{{ end }}",
            "{{ if gt .Ahead 0 }}#744d89{{ end }}",
            "{{ if gt .Behind 0 }}#744d89{{ end }}"
          ]
        },
        {
          "background": "#FFDE57",
          "foreground": "#111111",
          "powerline_symbol": "",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "style": "powerline",
          "template": "  {{ if .Venv }}{{ .Venv }}{{ end }} ",
          "type": "python"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin"
          },
          "template": "  {{ .FormattedMs }} ",
          "foreground": "#585858",
          "powerline_symbol": "",
          "background": "#e4e4e4",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "trailing_diamond": "",
          "template": "  {{ .CurrentDate | date .Format }} ",
          "foreground": "#f2f3f8",
          "background": "#d75f00",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "╰─ {{ if .Root }}#{{else}}${{end}}",
          "foreground": "#d75f00",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
